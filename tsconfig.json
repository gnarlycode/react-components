{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["dom", "es6", "es2015", "es2017", "es2018"],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "strictFunctionTypes": false,
    "target": "es5",
    "strict": true
  }
}
